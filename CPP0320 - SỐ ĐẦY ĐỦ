#include <iostream>
#include <algorithm>
#include <set>
#include <string>
#include <map>
using namespace std;
;
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        map<int, int> mp;
        string s1;
        cin >> s1;
        int check = 1, sum = 0;
        if (s1[0] == '0')
        {
            check = 0;
        }
        for (int i = 0; i < s1.size(); i++)
        {
            if (s1[i] < '0' || s1[i] > '9')
            {
                check = 0;
                break;
            }
            else
                mp[s1[i] - '0']++;
        }
        for (int i = 0; i <= 9; i++)
        {
            if (mp[i] > 0)
            {
                ++sum;
            }
        }
        if (check == 0)
        {
            cout << "INVALID" << endl;
        }
        else
        {
            if (sum == 10)
            {
                cout << "YES\n";
            }
            else
            {
                cout << "NO\n";
            }
        }
    }
}
