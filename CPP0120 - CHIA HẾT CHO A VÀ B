#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a, b, c, d, sum = 0;
        cin >> a >> b >> c >> d;
        for (int i = a; i <= b; i++)
        {
            if (i % c == 0 || i % d == 0)
            {
                ++sum;
            }
        }
        cout << sum << endl;
    }
}
