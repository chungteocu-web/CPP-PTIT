#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
#include <map>
using namespace std;
int maxx(int a, int b)
{
    if (a < b)
        return b;
    return a;
}
void daycon(int a[], int b[], int n)
{
    int max = 1;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j <= i; j++)
        {
            if (a[j] < a[i])
            {
                b[i] = maxx(b[j] + 1, b[i]);
            }
        }
    }
    for (int i = 0; i < n; i++)
    {
        if (b[i] > max)
        {
            max = b[i];
        }
    }
    cout << max << endl;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int n;
        cin >> n;
        int a[n], b[n];
        for (int i = 0; i < n; i++)
        {
            b[i] = 1;
        }
        for (int i = 0; i < n; i++)
        {
            cin >> a[i];
        }
        daycon(a, b, n);
    }
}
