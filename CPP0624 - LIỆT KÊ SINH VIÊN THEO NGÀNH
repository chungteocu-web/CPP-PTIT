#include <iostream>
#include <map>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
class SinhVien
{
public:
    string masv, ten, lop, email, nganh;
    friend istream &operator>>(istream &in, SinhVien &a)
    {
        in >> ws;
        getline(in, a.masv);
        getline(in, a.ten);
        getline(in, a.lop);
        getline(in, a.email);
        a.nganh = a.masv.substr(3, 4);
        return in;
    }
    friend ostream &operator<<(ostream &out, const SinhVien &a)
    {
        out << a.masv << " " << a.ten << " " << a.lop << " " << a.email << endl;
        return out;
    }
    friend bool cmp(const SinhVien &a, const SinhVien &b);
};
bool cmp(const SinhVien &a, const SinhVien &b)
{
    return a.masv < b.masv;
}
void nhapmap(map<string, string> &mp)
{
    mp["Ke toan"] = "DCKT";
    mp["Cong nghe thong tin"] = "DCCN";
    mp["An toan thong tin"] = "DCAT";
    mp["Vien thong"] = "DCVT";
    mp["Dien tu"] = "DCDT";
}
int main()
{
    map<string, string> mp;
    nhapmap(mp);
    int n, m;

    cin >> n;
    SinhVien ds[n];
    for (int i = 0; i < n; i++)
    {
        cin >> ds[i];
    }
    cin >> m;
    cin.ignore();
    while (m--)
    {
        string s, tmp;
        getline(cin, s);
        for (int i = 0; i < s.size(); i++)
        {
            tmp += toupper(s[i]);
        }

        cout << "DANH SACH SINH VIEN NGANH " << tmp << ":" << endl;
        for (int i = 0; i < n; i++)
        {
            if (ds[i].nganh == mp[s])
            {
                if ((ds[i].nganh == "DCCN" || ds[i].nganh == "DCAT") && ds[i].lop[0] == 'E')
                {
                    continue;
                }
                cout << ds[i];
            }
        }
    }
}
