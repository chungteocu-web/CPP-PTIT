#include <iostream>
#include <algorithm>
#include <string>
using namespace std;
void xuly(string s1)
{
    int n = s1.size();
    for (int i = 0; i < n; i++)
    {
        int check = 1;
        for (int j = 0; j < i; j++)
        {
            if (s1[i] == s1[j])
            {
                check = 0;
                break;
            }
        }
        for (int j = i + 1; j < n; j++)
        {
            if (s1[i] == s1[j])
            {
                check = 0;
                break;
            }
        }
        if (check == 1)
        {
            cout << s1[i];
        }
    }
    cout << endl;
}
int main()
{
    int m;
    cin >> m;
    cin.ignore(1);
    while (m--)
    {
        string s1;
        cin >> s1;
        xuly(s1);
    }
}
