#include <iostream>
#include <algorithm>
#include <vector>
#include <cmath>
#include <map>
using namespace std;
void hoanvi(int a[], int n)
{
    int vitri = 0;
    for (int i = n - 1; i > 0; i--)
    {
        if (a[i] < a[i - 1])
        {
            vitri = i - 1;
            break;
        }
    }
    int max = 0, vitri2;
    for (int i = vitri + 1; i < n; i++)
    {
        if (a[i] > max)
        {
            max = a[i];
            vitri2 = i;
        }
    }
    swap(a[vitri], a[vitri2]);
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int n;
        cin >> n;
        int a[n];
        for (int i = 0; i < n; i++)
        {
            cin >> a[i];
        }
        hoanvi(a, n);
        for (int i = 0; i < n; i++)
        {
            cout << a[i] << " ";
        }
        cout << endl;
    }
}
