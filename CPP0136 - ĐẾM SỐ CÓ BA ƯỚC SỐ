#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int check(long long m)
{
    if (m == 2 || m == 3)
    {
        return 1;
    }
    if (m < 3 || m % 2 == 0 || m % 3 == 0)
    {
        return 0;
    }
    for (long long i = 5; i * i <= m; i += 6)
    {
        if (m % i == 0 || m % (i + 2) == 0)
        {
            return 0;
        }
    }
    return 1;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        long long a;
        cin >> a;
        int sum = 0;
        for (long long i = 1; i <= a; i++)
        {
            if (check(i))
            {
                if (i * i > a)
                    break;
                ++sum;
            }
        }
        cout << sum << endl;
    }
}
