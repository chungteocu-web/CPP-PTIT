#include <iostream>
#include <algorithm>
using namespace std;
void xuly(int a, int b[])
{
    int max = 0;
    for (int i = 0; i < a; i++)
    {
        if (b[i] > max)
        {
            max = b[i];
        }
    }
    cout << max << endl;
}

int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a;
        cin >> a;
        int b[a];
        for (int i = 0; i < a; i++)
        {
            cin >> b[i];
        }
        xuly(a, b);
    }
}
