#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
#include <map>
#include <cmath>
using namespace std;

void daycon(long long mang[], int a)
{

    long long maxht = 0;
    for (int i = 0; i < a; i++)
    {
        long long tong = 1;
        for (int j = i; j < a; j++)
        {
            tong *= mang[j];
            if (tong > maxht)
            {
                maxht = tong;
            }
        }
    }
    cout << maxht << endl;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a;
        cin >> a;
        long long mang[a];
        for (int i = 0; i < a; i++)
        {
            cin >> mang[i];
        }
        daycon(mang, a);
    }
}
