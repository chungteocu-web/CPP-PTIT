#include <iostream>
#include <string>
#include <cmath>
using namespace std;
int main()
{
    int c, count = 0;
    cin >> c;
    while (c--)
    {
        string a;
        cin >> a;
        int n = a.size(), check = 0;
        for (int i = 1; i < n - 1; i++)
        {
            if (abs(a[i] - a[i - 1]) != 1 || abs(a[i + 1] - a[i]) != 1)
            {
                ++check;
            }
        }
        if (check == 0)
        {
            cout << "YES" << endl;
        }
        else
        {
            cout << "NO" << endl;
        }
    }
}
