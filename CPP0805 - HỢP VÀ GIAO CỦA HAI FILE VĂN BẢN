#include <iostream>
#include <fstream>
#include <string>
#include <set>
#include <vector>
#include <algorithm>
using namespace std;
int main()
{
    ifstream f1("DATA1.in");
    ifstream f2("DATA2.in");
    string x;
    set<string> st, sc;
    vector<string> s1, s2;
    while (f1 >> x)
    {
        for (int i = 0; i < x.length(); i++)
        {
            x[i] = tolower(x[i]);
        }
        s1.push_back((x));
        st.insert(x);
    }
    while (f2 >> x)
    {
        for (int i = 0; i < x.length(); i++)
        {
            x[i] = tolower(x[i]);
        }
        s2.push_back(x);
        st.insert(x);
    }
    for (auto it : st)
    {
        cout << it << " ";
    }
    cout << endl;
    for (int i = 0; i < s1.size(); i++)
    {
        for (int j = 0; j < s2.size(); j++)
        {
            if (s1[i] == s2[j])
            {
                sc.insert(s1[i]);
                break;
            }
        }
    }
    for (auto it : sc)
    {
        cout << it << " ";
    }
}
