#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
using namespace std;
long long ucln(long long a, long long b)
{
    while (b != 0)
    {
        long long tmp = a % b;
        a = b;
        b = tmp;
    }
    return a;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        string a, su;
        long long x, y;
        cin >> a >> x >> y;
        long long sum = ucln(x, y);
        while (sum--)
        {
            su += a;
        }
        cout << su << endl;
    }
}
