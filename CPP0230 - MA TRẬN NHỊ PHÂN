#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
void xuly(vector<vector<int>> &matran, int a)
{
    int sum = 0;
    for (int i = 0; i < a; i++)
    {
        int khong = 0, mot = 0;
        for (int j = 0; j < 3; j++)
        {
            if (matran[i][j] == 1)
            {
                ++mot;
            }
            else
            {
                ++khong;
            }
        }
        if (khong < mot)
        {
            ++sum;
        }
    }
    cout << sum;
}
void nhap(vector<vector<int>> &matran, int a)
{
    for (int i = 0; i < a; i++)
    {
        for (int j = 0; j < 3; j++)
        {
            cin >> matran[i][j];
        }
    }
}
int main()
{
    int a;
    cin >> a;
    vector<vector<int>> matran(a, vector<int>(3));
    nhap(matran, a);
    xuly(matran, a);
    cout << "\n";
}
