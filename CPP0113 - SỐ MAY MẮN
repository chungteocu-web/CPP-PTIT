#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
    int c;
    cin >> c;
    while (c--)
    {
        int a,check=0;
        cin >> a;
        for (int i = 0; i < 2; i++)
        {
            if (i == 0)
            {
                if (a % 10 != 6)
                {
                    check++;
                }
            }
            else if (i == 1)
            {
                if (a % 10 != 8)
                {
                    check++;
                }
            }
            a /= 10;
        }
        if (check == 0)
        {
            cout << 1 << endl;
        }
        else
        {
            cout << 0 << endl;
        }
    }
}
