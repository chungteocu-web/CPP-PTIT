#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
using namespace std;
int main()
{
    int m, n;
    cin >> m;
    while (m--)
    {
        int check = 0, vitri = 0;
        string s1, s2;
        char max = -1;
        cin >> s1;
        s2 = s1;
        for (int i = s1.size() - 1; i > 0; i--)
        {
            if (s1[i] < s1[i - 1])
            {
                check = i - 1;
                break;
            }
        }
        for (int i = check + 1; i < s1.size(); i++)
        {
            if (s1[check] > s1[i])
            {
                if (s1[i] > max)
                {
                    max = s1[i];
                }
            }
        }
        for (int i = check + 1; i < s1.size(); i++)
        {
            if (s1[i] == max)
            {
                vitri = i;
                break;
            }
        }
        swap(s1[check], s1[vitri]);
        if (s1 != s2)
            cout << s1 << endl;
        else
            cout << -1 << endl;
    }
}
