#include <iostream>
#include <string>
#include <map>
using namespace std;
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        string s1;
        cin >> s1;
        int le = 0, chan = 0;
        for (int i = 0; i < s1.size(); i++)
        {
            int tmp = s1[i] - '0';
            if (i % 2 != 0)
            {
                chan += tmp;
            }
            else
            {
                le += tmp;
            }
        }
        if (chan - le == 0 || (chan - le) % 11 == 0)
        {
            cout << 1 << endl;
        }
        else
        {
            cout << 0 << endl;
        }
    }
}
