#include <iostream>
#include <string>
#include <map>
using namespace std;
int main()
{
    int a, sum = 0;
    cin >> a;
    map<string, int> b;
    string s1;
    cin.ignore();
    for (int i = 0; i < a; i++)
    {
        getline(cin, s1);
        if (b[s1] == 0)
        {
            sum++;
        }
        b[s1]++;
    }
    cout << sum;
}
