#include <iostream>
#include <algorithm>
#include <set>
#include <string>
#include <sstream>
using namespace std;
string cong(string a)
{
    long long sum = 0;
    for (int i = 0; i < a.size(); i++)
    {
        sum += (a[i] - '0');
    }
    string x = to_string(sum);
    return x;
}
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        string so, sum;
        cin >> so;
        sum = cong(so);
        while (sum.size() >= 2)
        {
            sum = cong(sum);
        }
        int k = stoi(sum);
        if (k == 9)
        {
            cout << 1 << endl;
        }
        else
        {
            cout << 0 << endl;
        }
    }
}
