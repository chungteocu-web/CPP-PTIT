#include <iostream>
#include <string>
#include <sstream>
#include <map>
using namespace std;
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        map<char, int> ss;
        string s1;
        cin >> s1;
        for (int i = 0; i < s1.size(); i++)
        {
            ss[s1[i]]++;
        }
        for (int i = 0; i < s1.size(); i++)
        {
            int check = 1;
            for (int j = 0; j < i; j++)
            {
                if (s1[i] == s1[j])
                {
                    check = 0;
                    break;
                }
            }
            if (check == 1)
            {
                cout << s1[i] << ss[s1[i]];
            }
        }
        cout << endl;
    }
}
