#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
void xuly(int a, vector<int> b, int c)
{
    c = c % a;
    reverse(b.begin(), b.begin() + c);
    reverse(b.begin() + c, b.end());
    reverse(b.begin(), b.end());
    for (int a : b)
    {
        cout << a << " ";
    }
    cout << endl;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a, c;
        cin >> a >> c;
        vector<int> b(a);
        for (int i = 0; i < a; i++)
        {
            cin >> b[i];
        }
        xuly(a, b, c);
    }
}
