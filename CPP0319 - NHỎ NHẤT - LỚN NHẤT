#include <iostream>
#include <string>
#include <map>
using namespace std;
int minso(int b)
{
    if (9 > b)
    {
        return b;
    }
    return 9;
}
int main()
{
    int m, s;
    cin >> m >> s;
    if (m == 1 && s == 0)
    {
        cout << "0 0";
    }
    else if (s == 0 || s > 9 * m)
    {
        cout << "-1 -1";
    }
    else
    {
        int s1[100], s2[100], tmp = s - 1;
        for (int i = m - 1; i >= 0; i--)
        {
            s2[i] = minso(tmp);
            tmp = tmp - s2[i];
        }
        s2[0]++;
        for (int i = 0; i < m; i++)
        {
            cout << s2[i];
        }
        cout << " ";
        for (int i = 0; i < m; i++)
        {
            s1[i] = minso(s);
            s = s - s1[i];
        }
        for (int i = 0; i < m; i++)
        {
            cout << s1[i];
        }
    }
}
