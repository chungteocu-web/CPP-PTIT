#include <iostream>
#include <string>
#include <sstream>
#include <vector>
#include <algorithm>
#include <map>
using namespace std;
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        int n, k, x;
        map<int, int> mp;
        cin >> n >> k;
        vector<int> a, b;
        for (int i = 0; i < n; i++)
        {
            cin >> x;
            mp[x]++;
            if (mp[x] == 1)
                a.push_back(x);
        }
        for (int i = 0; i < k; i++)
        {
            cin >> x;
            if (mp[x] == 0)
            {
                a.push_back(x);
            }
            if (mp[x] > 0)
            {
                b.push_back(x);
            }
        }
        sort(a.begin(), a.end());
        sort(b.begin(), b.end());
        for (int i = 0; i < a.size(); i++)
        {
            cout << a[i] << " ";
        }
        cout << endl;
        for (int i = 0; i < b.size(); i++)
        {
            cout << b[i] << " ";
        }
        cout << endl;
    }
}
