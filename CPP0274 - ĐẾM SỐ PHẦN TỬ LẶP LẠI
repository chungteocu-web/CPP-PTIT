#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
void xuly(vector<int> &matran, int a)
{
    int count = 0;
    for (int i = 0; i < a; i++)
    {
        int check = 1;
        for (int j = 0; j < i; j++)
        {
            if (matran[i] == matran[j])
            {
                check = 0;
                break;
            }
        }
        if (check == 1)
        {
            int dem = 1;
            for (int j = i + 1; j < a; j++)
            {
                if (matran[i] == matran[j])
                {
                    ++dem;
                }
            }
            if (dem != 1)
            {
                count += dem;
            }
        }
    }
    cout << count << endl;
}
void nhap(vector<int> &matran, int a)
{
    for (int i = 0; i < a; i++)
    {
        cin >> matran[i];
    }
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a;
        cin >> a;
        vector<int> matran(a);
        nhap(matran, a);
        xuly(matran, a);
    }
}
