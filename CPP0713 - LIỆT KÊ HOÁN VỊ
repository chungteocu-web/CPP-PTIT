#include <iostream>
#include <algorithm>
#include <vector>
#include <cmath>
using namespace std;
void sinh(int a[], int n)
{
    int check = 1;
    for (int j = 0; j < n; j++)
    {
        a[j] = j + 1;
    }
    while (check == 1)
    {
        int i = n - 2;
        for (int j = 0; j < n; j++)
        {
            cout << a[j];
        }
        cout << " ";
        while (a[i] > a[i + 1] && i >= 0)
        {
            i--;
        }
        if (i < 0)
        {
            check = 0;
        }
        else
        {
            int vitri = 0, min = 999999;
            for (int j = i + 1; j < n; j++)
            {
                if (a[j] > a[i] && a[j] < min)
                {
                    min = a[j];
                    vitri = j;
                }
            }
            swap(a[i], a[vitri]);
            reverse(a + i + 1, a + n);
        }
    }
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int n;
        cin >> n;
        int a[n];
        sinh(a, n);
        cout << endl;
    }
}
